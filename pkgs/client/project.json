{
  "name": "client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "pkgs/client/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "supabase:ci-marker": {
      "executor": "nx:noop"
    },
    "build": {
      "executor": "nx:noop",
      "inputs": ["production", "^production"],
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build", "build:lib", "build:browser"]
    },
    "verify-exports": {
      "executor": "nx:run-commands",
      "inputs": [
        "{projectRoot}/scripts/verify-exports.js",
        "{projectRoot}/package.json"
      ],
      "options": {
        "cwd": "{projectRoot}",
        "command": "pnpm run verify-exports"
      },
      "dependsOn": ["build"]
    },
    "build:lib": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "options": {
        "outputPath": "pkgs/client/dist",
        "configFile": "pkgs/client/vite.config.ts"
      }
    },
    "build:browser": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build", "build:lib"],
      "inputs": ["production", "^production"],
      "options": {
        "outputPath": "pkgs/client/dist",
        "configFile": "pkgs/client/vite.browser.config.ts"
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "dependsOn": ["^build"],
      "inputs": ["default", "^production"],
      "options": {
        "noEmit": true,
        "tsConfig": "pkgs/client/tsconfig.lib.json",
        "main": "pkgs/client/src/index.ts",
        "outputPath": "pkgs/client/dist"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "inputs": ["default", "^production"]
    },
    "supabase:ensure-started": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["supabase:prepare"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["../../scripts/supabase-start-locked.sh ."],
        "parallel": false
      }
    },
    "supabase:start": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["../../scripts/supabase-start-locked.sh ."],
        "parallel": false
      }
    },
    "supabase:stop": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["supabase stop --no-backup"],
        "parallel": false
      }
    },
    "supabase:status": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["supabase status"],
        "parallel": false
      }
    },
    "supabase:prepare": {
      "executor": "nx:run-commands",
      "local": true,
      "dependsOn": ["^verify-migrations"],
      "inputs": [
        "{workspaceRoot}/pkgs/core/supabase/migrations/**/*.sql",
        "{workspaceRoot}/pkgs/core/supabase/seed.sql"
      ],
      "outputs": [
        "{projectRoot}/supabase/migrations",
        "{projectRoot}/supabase/seed.sql"
      ],
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "mkdir -p supabase/migrations/",
          "rm -f supabase/migrations/*.sql",
          "cp ../core/supabase/migrations/*.sql supabase/migrations/",
          "cp ../core/supabase/seed.sql supabase/"
        ],
        "parallel": false
      }
    },
    "supabase:reset": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["supabase:prepare"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh .",
          "supabase db reset"
        ],
        "parallel": false
      }
    },
    "test:integration": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["supabase:prepare", "build"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh .",
          "psql 'postgresql://postgres:postgres@localhost:50522/postgres' -c 'SELECT pgflow_tests.reset_db()'",
          "vitest run __tests__/integration/"
        ],
        "parallel": false
      }
    },
    "test:unit": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "inputs": ["default", "^production"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["vitest run __tests__/ --exclude __tests__/integration/"],
        "parallel": false
      }
    },
    "test:vitest": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["supabase:prepare", "build"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh .",
          "psql 'postgresql://postgres:postgres@localhost:50522/postgres' -c 'SELECT pgflow_tests.reset_db()'",
          "vitest run __tests__/"
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:noop",
      "inputs": ["default", "^production"],
      "dependsOn": ["test:vitest", "test:types"]
    },
    "benchmark": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["supabase:prepare", "build"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh .",
          "psql 'postgresql://postgres:postgres@localhost:50522/postgres' -c 'SELECT pgflow_tests.reset_db()'",
          "node scripts/performance-benchmark.mjs"
        ],
        "parallel": false
      }
    },
    "test:types:vitest": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["production", "^production"],
      "dependsOn": ["^build", "lint"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "pnpm vitest --typecheck.only --run --config vitest.typecheck.config.ts"
      }
    },
    "test:types:strict": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["production", "^production"],
      "dependsOn": ["^build"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "bash ../../scripts/typecheck-ts2578.sh"
      }
    },
    "test:types": {
      "executor": "nx:noop",
      "dependsOn": ["test:types:vitest", "test:types:strict"]
    },
    "prepush": {
      "executor": "nx:noop",
      "dependsOn": [
        "lint",
        "build",
        "typecheck",
        "test:types:vitest",
        "test:types:strict"
      ]
    }
  }
}
