{
  "name": "edge-worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "pkgs/edge-worker",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "dependsOn": ["^build"]
    },
    "_build_disabled": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "pkgs/edge-worker/dist",
        "main": "pkgs/edge-worker/src/index.ts",
        "tsConfig": "pkgs/edge-worker/tsconfig.lib.json",
        "assets": ["pkgs/edge-worker/*.md"],
        "rootDir": "pkgs/edge-worker/src"
      }
    },
    "test:nx-deno": {
      "executor": "@axhxrx/nx-deno:test",
      "local": true,
      "outputs": ["{workspaceRoot}/coverage/pkgs/edge-worker"],
      "options": {
        "coverageDirectory": "coverage/pkgs/edge-worker",
        "denoConfig": "pkgs/edge-worker/deno.test.json",
        "allowNone": false,
        "check": "local"
      }
    },
    "lint:deno": {
      "executor": "@axhxrx/nx-deno:lint",
      "cache": true,
      "local": true,
      "inputs": ["default", "{projectRoot}/deno.test.json"],
      "options": {
        "denoConfig": "pkgs/edge-worker/deno.test.json",
        "ignore": "pkgs/edge-worker/supabase/functions/_dist/"
      }
    },
    "lint:jsr": {
      "executor": "nx:run-commands",
      "cache": true,
      "local": true,
      "inputs": ["{projectRoot}/src/**/*.ts"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "command": "npx eslint 'src/**/*.ts' --rule 'no-restricted-syntax: [\"error\", {\"selector\": \"TSModuleDeclaration[global=true]\", \"message\": \"JSR forbids global type augmentation (declare global). This will cause JSR publish to fail.\"}]'"
      }
    },
    "lint": {
      "executor": "nx:noop",
      "dependsOn": ["lint:deno", "lint:jsr"]
    },
    "supabase:ensure-started": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh ."
        ],
        "parallel": false
      }
    },
    "supabase:start": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["../../scripts/supabase-start-locked.sh ."],
        "parallel": false
      }
    },
    "supabase:stop": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": ["supabase stop --no-backup"],
        "parallel": false
      }
    },
    "supabase:status": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": ["supabase status"],
        "parallel": false
      }
    },
    "supabase:restart": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": ["supabase stop --no-backup", "supabase start"],
        "parallel": false
      }
    },
    "supabase:reset": {
      "dependsOn": ["^verify-migrations"],
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "mkdir -p supabase/migrations/",
          "rm -f supabase/migrations/*.sql",
          "cp ../core/supabase/migrations/*.sql supabase/migrations/",
          "../../scripts/supabase-start-locked.sh .",
          "supabase db reset"
        ],
        "parallel": false
      }
    },
    "supabase:functions-serve": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "../../scripts/supabase-start-locked.sh .",
          "supabase functions serve --env-file supabase/functions/.env --no-verify-jwt"
        ],
        "parallel": false
      }
    },
    "db:ensure": {
      "executor": "nx:run-commands",
      "local": true,
      "cache": false,
      "dependsOn": ["^verify-migrations"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": ["./scripts/ensure-db"],
        "parallel": false
      }
    },
    "test:unit": {
      "dependsOn": ["db:ensure", "^build"],
      "executor": "nx:run-commands",
      "local": true,
      "inputs": ["default", "^production"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": [
          "deno test --config deno.test.json --allow-all --env=supabase/functions/.env tests/unit/"
        ],
        "parallel": false
      }
    },
    "test:integration": {
      "dependsOn": ["db:ensure", "^build"],
      "executor": "nx:run-commands",
      "local": true,
      "inputs": ["default", "^production"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": [
          "deno test --config deno.test.json --allow-all --env=supabase/functions/.env tests/integration/"
        ],
        "parallel": false
      }
    },
    "sync-e2e-deps": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "local": true,
      "inputs": ["^production"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": ["./scripts/sync-e2e-deps.sh"],
        "parallel": false
      }
    },
    "prepare-e2e": {
      "executor": "nx:run-commands",
      "dependsOn": ["sync-e2e-deps", "^verify-migrations"],
      "local": true,
      "cache": false,
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "mkdir -p supabase/migrations/",
          "rm -f supabase/migrations/*.sql",
          "cp ../core/supabase/migrations/*.sql supabase/migrations/",
          "../../scripts/supabase-start-locked.sh ."
        ],
        "parallel": false
      }
    },
    "serve:functions:e2e": {
      "executor": "nx:run-commands",
      "continuous": true,
      "dependsOn": ["prepare-e2e"],
      "local": true,
      "cache": false,
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": [
          "supabase functions serve --env-file supabase/functions/.env --import-map supabase/functions/deno.json --no-verify-jwt"
        ],
        "parallel": false
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "dependsOn": ["prepare-e2e", "serve:functions:e2e"],
      "local": true,
      "inputs": ["default", "^production"],
      "options": {
        "cwd": "pkgs/edge-worker",
        "commands": [
          "timeout 30 bash -c 'echo \"Waiting for functions server (port 50321)...\"; until nc -z localhost 50321 2>/dev/null; do sleep 0.5; done; echo \" Ready!\"'",
          "deno test --config deno.test.json --allow-all --env=supabase/functions/.env tests/e2e/"
        ],
        "parallel": false
      }
    },
    "test": {
      "inputs": ["default", "^production"],
      "dependsOn": ["test:types", "test:unit", "test:integration"]
    },
    "test:types": {
      "executor": "nx:run-commands",
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "deno check --config deno.test.json src/examples/*.example.ts tests/types/*.test-d.ts"
      }
    },
    "prepush": {
      "executor": "nx:noop",
      "dependsOn": ["lint", "build", "test:types"]
    }
  },
  "tags": []
}
