{
  "name": "@pgflow/edge-worker",
  "version": "0.0.4",
  "license": "MIT",
  "exports": "./mod.ts",
  "imports": {
    "@henrygd/queue": "jsr:@henrygd/queue@^1.0.7",
    "@std/assert": "jsr:@std/assert",
    "@std/async": "jsr:@std/async",
    "@std/log": "jsr:@std/log@^0.224.13",
    "@std/testing/mock": "jsr:@std/testing/mock",
    "postgres": "npm:postgres@3.4.5"
  },
  "tasks": {
    "db:clean": "cd tests/db && docker compose down --volumes && rm *.sql || true",
    "db:prepare": "./scripts/concatenate-migrations.sh",
    "db:run": "cd tests/db && docker compose up --detach && ../../scripts/wait-for-localhost 5432",
    "db:ensure": "deno task db:clean && deno task db:prepare && deno task db:run",
  }
}
