{
  "name": "@pgflow/supaworker-tests",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "supaworker-tests",
    "projectType": "application",
    "sourceRoot": "pkgs/supaworker-tests/src",
    "targets": {
      "test": {
        "executor": "@nx/vite:test",
        "outputs": [
          "{workspaceRoot}/coverage/pkgs/supaworker-tests"
        ],
        "options": {
          "passWithNoTests": false,
          "reportsDirectory": "../../coverage/pkgs/supaworker-tests"
        }
      }
    }
  },
  "devDependencies": {
    "supabase": "^2.2.1"
  }
}
