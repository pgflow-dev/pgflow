{
  "name": "backend",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "(pnpm status || pnpm restart) && pnpm functions",
    "gen-types": "bin/supabase gen types typescript --local --schema public --schema acl --schema auth --schema acl --schema storage --schema chat --schema feed --schema pgflow --schema pgflow_locks > types.d.ts",
    "deno": "deno cache --config functions/deno.json functions/**/*.ts flows/**/*.ts",
    "db-reset": "bin/supabase db reset && pnpm gen-types && bin/load-secrets",
    "db-migrate": "bin/supabase migration up && pnpm gen-types",
    "functions": "bin/supabase functions serve --env-file .env",
    "restart": "pnpm stop && pnpm start",
    "status": "bin/supabase status",
    "stop": "bin/supabase stop",
    "start": "bin/supabase start",
    "test": "bin/test",
    "test:watch": "ls tests/**/*.sql | entr -rc bin/test",
    "pgflow:gen-schema": "pnpm tsx --tsconfig schema-generator/tsconfig.json schema-generator/generate-schema.ts"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@types/json-schema": "^7.0.15",
    "groq-sdk": "^0.7.0",
    "json-schema": "^0.4.0",
    "openai": "^4.71.1",
    "supabase": "^2.0.2",
    "ts-json-schema-generator": "^2.3.0",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "zod": "^3.23.8"
  }
}
