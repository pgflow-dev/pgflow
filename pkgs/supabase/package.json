{
  "name": "backend",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "(pnpm status || pnpm restart) && pnpm functions",
    "gen-types": "bin/supabase gen types typescript --local --schema public --schema acl --schema auth --schema acl --schema storage --schema chat --schema feed > types.d.ts",
    "deno": "deno cache --config functions/deno.json functions/**/index.ts",
    "db-reset": "bin/supabase db reset && pnpm gen-types",
    "db-migrate": "bin/supabase migration up && pnpm gen-types",
    "functions": "bin/supabase functions serve --env-file .env",
    "restart": "pnpm stop && pnpm start",
    "status": "bin/supabase status",
    "stop": "bin/supabase stop",
    "start": "bin/supabase start"
  },
  "devDependencies": {
    "supabase": "^1.204.3"
  }
}
