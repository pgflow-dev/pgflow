{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".svelte-kit/**",
        ".vercel_build_output/**",
        ".vercel/**",
        "build/**"
      ],
      "inputs": ["src/**/*", "pkgs/supabase/types.d.ts"]
    },
    "check": {
      "dependsOn": []
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    }
  }
}
