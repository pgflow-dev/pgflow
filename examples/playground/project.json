{
  "name": "playground",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/playground",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "netlify build --context=${NETLIFY_CONTEXT:-production}",
        "cwd": "{workspaceRoot}"
      },
      "inputs": [
        "production",
        "^production",
        {
          "env": "NEXT_PUBLIC_SUPABASE_URL"
        },
        {
          "env": "NEXT_PUBLIC_SUPABASE_ANON_KEY"
        },
        {
          "externalDependencies": ["netlify-cli"]
        }
      ],
      "outputs": [
        ".netlify",
        "{projectRoot}/.next"
      ]
    },
    "deploy:preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{workspaceRoot}",
        "command": "netlify deploy --dir=.netlify --functions=.netlify/functions --filter examples/playground --alias=${GITHUB_SHA:-preview}",
        "parallel": false
      }
    },
    "deploy:prod": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{workspaceRoot}",
        "command": "netlify deploy --dir=.netlify --functions=.netlify/functions --filter examples/playground --prod",
        "parallel": false
      }
    }
  }
}
