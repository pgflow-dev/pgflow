{
  "name": "playground",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/playground",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next build",
        "cwd": "{projectRoot}"
      },
      "inputs": [
        "production",
        "^production",
        {
          "env": "NEXT_PUBLIC_SUPABASE_URL"
        },
        {
          "env": "NEXT_PUBLIC_SUPABASE_ANON_KEY"
        }
      ],
      "outputs": [
        "{projectRoot}/.next"
      ]
    },
    "deploy:preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "pnpm netlify deploy --dir=.netlify --functions=.netlify/functions --alias=${GITHUB_SHA:-preview}",
        "parallel": false
      }
    },
    "deploy:prod": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "pnpm netlify deploy --dir=.netlify --functions=.netlify/functions --prod",
        "parallel": false
      }
    }
  }
}
